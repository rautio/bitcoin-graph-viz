{
  "name": "bitcoin-graph-viz",
  "version": "0.0.1",
  "description": "A graph visualization of bitcoin transactions",
  "main": "server.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "test:travis": "mocha --compilers js:babel-core/register ./test/*.spec.js",
    "cover": "babel-node node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha ./test/*.spec.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "lint": "esw ./",
    "lint:watch": "esw -w ./",
    "clean": "rimraf lib",
    "start": "npm-run-all --parallel lint:watch dev",
    "dev": "nodemon server.js --watch ./ --exec babel-node --presets es2015,stage-2",
    "prod": "babel-node server.js --presets es2015,stage-2",
    "doc": "jsdoc api -r -d doc/ --private --readme README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rautio/bitcoin-graph-viz.git"
  },
  "keywords": [
    "graph",
    "visualization",
    "bitcoin",
    "graph",
    "sigmajs"
  ],
  "author": "Oskari Rautiainen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rautio/bitcoin-graph-viz/issues"
  },
  "homepage": "https://github.com/rautio/bitcoin-graph-viz#readme",
  "devDependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "body-parser": "^1.18.2",
    "coveralls": "^3.0.0",
    "es6-promise": "^4.2.4",
    "eslint": "^4.17.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-watch": "^3.1.3",
    "expect": "^22.1.0",
    "exports-loader": "^0.7.0",
    "express": "^4.16.2",
    "fs": "0.0.1-security",
    "imports-loader": "^0.7.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.14.12",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "sigma": "^1.2.1",
    "winston": "^2.4.0"
  }
}
